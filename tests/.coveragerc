[run]
branch = True
source = src
source_pkgs = jmespath_filters
relative_files = True
# partial_branches =
omit =
    **/~*/*
    src/jmespath_filters/types.py
    src/jmespath_filters/version.py
disable_warnings =
    module-not-measured

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    pragma: no cover
    pragma: no-cover
    def __repr__
    pass
    if settings.DEBUG:
    if DEBUG
    if SENTRY_ENABLED
    if typing.TYPE_CHECKING
    if TYPE_CHECKING
    if self\.debug
    if __name__ == .__main__.:
    except JSONDecodeError
    except ValidationError
    except Exception
    raise AssertionError
    raise NotImplementedError
    except ImportError
    except BaseException as e
    raise CommandError.*
    self.halt\(.*
    @abc.abstractmethod
    @abstractmethod
    override = lambda _func: _func


fail_under=90
precision=2
ignore_errors = True

[html]
directory = ./~build/coverage
